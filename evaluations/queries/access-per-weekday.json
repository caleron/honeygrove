{
  "query": {
    "bool": {
      "must": [
        {
          "match": {
            "event_type": "login"
          }
        },
        {
          "match": {
            "service": "SSH"
          }
        },
        {
          "range": {
            "@timestamp": {
              "gte": "2019-04-15T00:00:00.000000",
              "lte": "2019-06-17T00:00:00.000000"
            }
          }
        }
      ]
    }
  },
  "aggs": {
    "attempts_per_weekday": {
      "terms": {
        "script": "doc['@timestamp'].value.getDayOfWeekEnum()"
      }
    }
  },
  "size": 0
}